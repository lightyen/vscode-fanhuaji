{
  "name": "vscode-fanhuaji",
  "displayName": "Fanhuaji",
  "description": "一個繁簡轉換與本地化工具",
  "version": "0.0.1",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/vscode-fanhuaji.git"
  },
  "publisher": "lightyen",
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "fanhuaji",
    "converter",
    "chinese"
  ],
  "engines": {
    "vscode": "^1.52.1"
  },
  "galleryBanner": {
    "color": "#323840",
    "theme": "dark"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "scripts": {
    "test": "jest",
    "doit": "yarn upgrade-interactive --latest",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts",
    "watch": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts --watch",
    "vscode:prepublish": "yarn build",
    "format": "prettier --write \"**/*.{ts,json,yml}\"",
    "check-format": "prettier --check \"**/*.{ts,json,yml}\"",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "jest": "^26.6.3",
    "js-yaml-loader": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.0.0",
    "ts-node": "^9.1.1",
    "ts-paths-resolve-plugin": "^5.2.2",
    "typescript": "^4.2.4",
    "vsce": "1.87.1",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {},
  "contributes": {
    "configuration": {
      "title": "Fanhuaji",
      "properties": {
        "fanhuaji.api.server": {
          "description": "%config.api.server%",
          "type": "string",
          "default": "https://api.zhconvert.org"
        },
        "fanhuaji.api.key": {
          "description": "%config.api.key%",
          "type": "string",
          "default": ""
        },
        "fanhuaji.api.convertParams": {
          "description": "%config.api.convertParams%",
          "type": "object",
          "default": {},
          "properties": {
            "ignoreTextStyles": {
              "type": "string"
            },
            "jpTextStyles": {
              "type": "string"
            },
            "modules": {
              "type": "object",
              "default": {}
            },
            "userPreReplace": {
              "type": "object",
              "default": {}
            },
            "userPostReplace": {
              "type": "object",
              "default": {}
            },
            "userProtectReplace": {
              "type": "array",
              "default": []
            }
          }
        }
      }
    },
    "commands": [
      {
        "command": "fanhuaji.serviceInfo",
        "title": "%command.fanhuaji.serviceInfo%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Traditional",
        "title": "%command.fanhuaji.Traditional%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Simplified",
        "title": "%command.fanhuaji.Simplified%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Taiwan",
        "title": "%command.fanhuaji.Taiwan%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.China",
        "title": "%command.fanhuaji.China%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Hongkong",
        "title": "%command.fanhuaji.Hongkong%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Pinyin",
        "title": "%command.fanhuaji.Pinyin%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Bopomofo",
        "title": "%command.fanhuaji.Bopomofo%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.Mars",
        "title": "%command.fanhuaji.Mars%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.WikiSimplified",
        "title": "%command.fanhuaji.WikiSimplified%",
        "category": "Fanhuaji"
      },
      {
        "command": "fanhuaji.WikiTraditional",
        "title": "%command.fanhuaji.WikiTraditional%",
        "category": "Fanhuaji"
      }
    ]
  }
}
